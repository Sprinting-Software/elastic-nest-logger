{
  "name": "sprinting-retail-common",
  "version": "0.0.1",
  "description": "Elastic Nest Logger",
  "author": "Nairi Abgaryan",
  "contributors": [
    "Nairi Abgaryan"
  ],
  "license": "MIT",
  "keywords": [
    "nest",
    "nestjs",
    "winston",
    "apm",
    "logger",
    "logging"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sprinting-Software/sprinting-retail-common"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier --write \"./**/*.ts\"",
    "lint": "tslint --fix ./**/*.ts -p tsconfig.json -c tslint.json",
    "build": "rimraf dist && tsc",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.3.1",
    "@nestjs/common": "^9.2.1",
    "elastic-apm-node": "^3.42.0",
    "jws": "^4.0.0",
    "rimraf": "^4.1.2",
    "tslint": "^6.1.3",
    "udp-transport-winston": "^1.2.11",
    "winston": "^3.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/node": "^18.11.18"
  }
}
